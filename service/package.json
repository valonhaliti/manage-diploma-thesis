{
  "name": "noderestshop",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "prestart": "npm run-script build",
    "dev": "nodemon src/app.js --exec babel-node",
    "start": "node lib/app.js",
    "test": "set NODE_ENV=test&&mocha --require @babel/polyfill --require @babel/register",
    "open:src": "node lib/app.js",
    "localtunnel": "lt --port 3001 --subdomain service",
    "build": "./node_modules/.bin/babel src --out-dir lib --require babel/polyfill",
    "share": "npm-run-all --parallel start localtunnel",
    "compile": "./node_modules/.bin/babel src --out-dir .compiled --source-maps --watch",
    "showerror": "jv error.log"
  },
  "author": "Valon Haliti",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "multer": "^1.3.1",
    "mysql": "^2.16.0",
    "string-similarity": "^2.0.0",
    "supertest": "^3.1.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "node-mocks-http": "^1.7.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "open": "0.0.5"
  }
}
